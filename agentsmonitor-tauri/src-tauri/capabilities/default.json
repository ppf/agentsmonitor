{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main", "popover"],
  "permissions": [
    "core:default",
    "core:event:default",
    "core:event:allow-listen",
    "core:event:allow-emit",
    "opener:default",
    "fs:default",
    "fs:read-all",
    "fs:write-all",
    "store:default",
    "notification:default",
    "dialog:default"
  ]
}
